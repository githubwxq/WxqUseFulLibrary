<?xml version="1.0" encoding="utf-8"?>


    <RelativeLayout
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tl="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".bmob.activity.BmobIndexActivity"
        >

        <com.example.wxq.wxqutilslibrary.widget.mulripletablayout.CommonTabLayout
            android:id="@+id/tl_bottom"
            android:layout_width="match_parent"
            android:layout_height="54dp"
            android:background="#ffffff"
            tl:tl_iconHeight="23dp"
            tl:tl_iconWidth="23dp"
            tl:tl_indicator_color="#ffffff"
            tl:tl_textSelectColor="#8888"
            tl:tl_textUnselectColor="#660000"
            tl:tl_underline_color="#9f9f"
            tl:tl_underline_height="0dp"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <FrameLayout
            android:id="@+id/fl_change"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_above="@+id/tl_bottom">
        </FrameLayout>
    </RelativeLayout>
